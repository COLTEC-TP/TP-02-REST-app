<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.ufmg.coltec.rest.ItemSelecionado">



    <TextView
        android:id="@+id/textView1"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text="TextView"
        android:textAlignment="center"
        android:paddingTop="30dp"
        android:textSize="25dp"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView1"
        android:layout_marginTop="45dp"
         />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_above="@+id/textView19"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="22dp"
        android:layout_marginStart="45dp"
        android:text="EUR"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/textView18"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_alignBaseline="@+id/textView15"
        android:layout_alignBottom="@+id/textView15"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="96dp"
        android:text=""
        android:textSize="20dp"/>

    <EditText
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView2"
        android:layout_alignStart="@+id/textView15"
        android:layout_marginBottom="27dp"
        android:text="1"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView18"
        android:layout_alignTop="@+id/textView19"
        android:text=""
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/textView21"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView19"
        android:layout_marginTop="14dp"
         />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_above="@+id/textView24"
        android:layout_alignStart="@+id/textView19"
        android:layout_marginBottom="20dp"
        android:text=""
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/textView23"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_alignBaseline="@+id/textView22"
        android:layout_alignBottom="@+id/textView22"
        android:layout_alignStart="@+id/textView20"
        android:text="EUR"
        android:textSize="20dp"/>

    <EditText
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView21"
        android:layout_alignStart="@+id/textView22"
        android:layout_marginBottom="25dp"
        android:text="1"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView24"
        android:layout_alignBottom="@+id/textView24"
        android:layout_alignStart="@+id/textView23"
        android:text=""
        android:textSize="20dp"/>

</RelativeLayout>
